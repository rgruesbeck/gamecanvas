{
    "pages": [],
    "routes": [],
    "deploy": {
        "subdomain": "game.withkoji.com",
        "frontend": {
            "output": "dist",
            "commands": ["npm install", "npm run build"],
            "injections": [{
                "paths": ["*.html"],
                "site": "<head>",
                "position": "after",
                "value": "<title>${metadata.title}</title>"
            }, {
                "paths": ["*.html"],
                "site": "<head>",
                "position": "after",
                "value": "<link rel=\"shortcut icon\" type=\"image/png\" href=\"/favicon.png\"/>"
            }, {
                "paths": ["*.html"],
                "site": "<head>",
                "position": "after",
                "value": "<meta property=\"og:type\" content=\"website\">"
            }, {
                "paths": ["*.html"],
                "site": "<head>",
                "position": "after",
                "value": "<meta property=\"og:title\" content=\"${metadata.title}\">"
            }, {
                "paths": ["*.html"],
                "site": "<head>",
                "position": "after",
                "value": "<meta property=\"og:description\" content=\"${metadata.description}\">"
            }, {
                "paths": ["*.html"],
                "site": "<head>",
                "position": "after",
                "value": "<meta property=\"og:image\" content=\"${metadata.image}\">"
            }]
        }
    },
    "backend": {},
    "develop": {
        "frontend": {
            "path": ".",
            "port": 1234,
            "events": {
                "started": "npm start",
                "building": "node config.js",
                "built": "Built in",
                "build-error": "npm ERR!"
            },
            "startCommand": "npm start"
        }
    },
    "general": {
        "name": "game",
        "startText": "Play",
        "winText": "You Win!",
        "gameoverText": "Game Over",
        "lives": "3"
    },
    "@@editor": [{
        "key": "general",
        "name": "Game settings",
        "icon": "üéÆ",
        "source": "general.json",
        "fields": [{
            "key": "name",
            "name": "App name"
        }, {
            "key": "startText",
            "name": "Start Button Text"
        }, {
            "key": "winText",
            "name": "Text to show when player wins"
        }, {
            "key": "gameoverText",
            "name": "Text to show when game over"
        }, {
            "key": "lives",
            "name": "Lives"
        }]
    }, {
        "key": "images",
        "name": "Images",
        "icon": "üñº",
        "source": "images.json",
        "fields": [{
            "key": "playerImage",
            "name": "Player",
            "description": "Player image",
            "type": "image"
        }, {
            "key": "backgroundImage",
            "name": "Background",
            "description": "Background image",
            "type": "image"
        }]
    }, {
        "key": "metadata",
        "name": "Metadata",
        "icon": "‚öô",
        "source": "metadata.json",
        "fields": [{
            "key": "title",
            "name": "Name for your App",
            "description": "Will show up as title on Google and Facebook"
        }, {
            "key": "description",
            "name": "Description",
            "description": "Description of your app for Google and Facebook",
            "type": "textarea"
        }, {
            "key": "gaCode",
            "name": "GA Tracking ID",
            "description": "Tracking ID for Google Analytics",
            "type": "text",
            "typeOptions": {
                "monospace": true
            }
        }, {
            "key": "image",
            "name": "Share Image",
            "type": "image",
            "description": "Embedded Image to load when your App is shared"
        }, {
            "key": "icon",
            "name": "Icon",
            "type": "image",
            "description": "Small Icon for your App"
        }]
    }, {
        "key": "sounds",
        "name": "Sounds",
        "icon": "üîäÔ∏è",
        "source": "sounds.json",
        "fields": [{
            "key": "backgroundMusic",
            "name": "Background Music",
            "description": "URL of audio file to loop in background",
            "type": "sound"
        }]
    }, {
        "key": "style",
        "name": "Colors",
        "icon": "üíÖ",
        "source": "style.json",
        "fields": [{
            "key": "textColor",
            "name": "Text color",
            "description": "Default color for text",
            "type": "color"
        }, {
            "key": "primaryColor",
            "name": "Primary Color",
            "description": "Primary Color",
            "type": "color"
        }, {
            "key": "backgroundColor",
            "name": "Background Color",
            "description": "Background Color",
            "type": "color"
        }, {
            "key": "fontFamily",
            "name": "Font Family",
            "description": "Google fonts link to font to use on page"
        }]
    }],
    "images": {
        "playerImage": "https://i.imgur.com/1VTl2r7.png",
        "backgroundImage": "https://i.imgur.com/2F4bpZT.png"
    },
    "metadata": {
        "title": "game",
        "description": "game description",
        "image": "",
        "icon": "",
        "gaCode": ""
    },
    "project": {
        "name": "game",
        "description": "game description",
        "version": "1.0.0"
    },
    "sounds": {
        "backgroundMusic": "https://s3-us-west-2.amazonaws.com/koji-cdn/media/ron/game-music.mp3"
    },
    "style": {
        "textColor": "purple",
        "primaryColor": "white",
        "backgroundColor": "purple",
        "fontFamily": "Audiowide"
    }
}